<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="768.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="vehiclesfxml.DashboardController">
  <children>
    <HBox fx:id="topHBox" layoutX="14.0" layoutY="37.0" prefHeight="129.0" prefWidth="986.0" style="-fx-border-color: #000;" />
    <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="1024.0">
      <menus>
        <Menu mnemonicParsing="false" onAction="#closeApplication" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Exit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#displayAboutInfo" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <Pane fx:id="paneCharts" layoutX="14.0" layoutY="175.0" prefHeight="474.0" prefWidth="461.0" style="-fx-border-color: #000;">
      <children>
        <VBox fx:id="vBoxLeft" prefHeight="76.0" prefWidth="461.0">
          <children>
            <VBox alignment="TOP_CENTER" prefHeight="49.0" prefWidth="461.0">
              <children>
                <Label prefHeight="25.0" prefWidth="242.0" text="Select the type of chart to display" textAlignment="CENTER">
                  <font>
                    <Font size="16.0" />
                  </font>
                </Label>
                <HBox fx:id="hBoxChartSelection" alignment="TOP_CENTER" prefHeight="2.0" prefWidth="461.0">
                  <children>
                    <RadioButton fx:id="rbPieChart" mnemonicParsing="false" selected="true" text="Pie">
                      <toggleGroup>
                        <ToggleGroup fx:id="toggleChart" />
                      </toggleGroup>
                      <HBox.margin>
                        <Insets />
                      </HBox.margin>
                    </RadioButton>
                    <RadioButton fx:id="rbBarChart" mnemonicParsing="false" text="Bar" toggleGroup="$toggleChart" />
                    <RadioButton fx:id="rbLineChart" mnemonicParsing="false" text="Line" toggleGroup="$toggleChart" />
                  </children>
                </HBox>
              </children>
            </VBox>
            <HBox fx:id="hBoxYearCheckboxes" alignment="CENTER" prefHeight="45.0" prefWidth="459.0">
              <children>
                <ComboBox fx:id="comboBoxYears" onAction="#setupPieChart" prefWidth="150.0">
                  <HBox.margin>
                    <Insets left="30.0" right="50.0" />
                  </HBox.margin>
                </ComboBox>
              </children>
            </HBox>
          </children>
        </VBox>
        <Pane fx:id="panePieChart" layoutY="80.0" prefHeight="398.0" prefWidth="461.0">
          <children>
            <PieChart fx:id="pieChart" prefHeight="383.0" prefWidth="459.0" />
          </children>
        </Pane>
        <Pane fx:id="paneBarChart" layoutY="80.0" prefHeight="398.0" prefWidth="461.0">
          <children>
            <BarChart fx:id="barChart" prefHeight="383.0" prefWidth="459.0" title="Bar Chart">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </BarChart>
          </children>
        </Pane>
        <Pane fx:id="paneLineChart" layoutY="80.0" prefHeight="398.0" prefWidth="461.0">
          <children>
            <LineChart fx:id="lineChart" prefHeight="383.0" prefWidth="459.0" title="Line Chart">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </LineChart>
          </children>
        </Pane>
      </children>
    </Pane>
    <Pane layoutX="37.0" layoutY="520.0" prefHeight="200.0" prefWidth="200.0">
      <children>
        <Pane layoutX="-21.0" layoutY="140.0" prefHeight="61.0" prefWidth="986.0" style="-fx-border-color: #000;" />
      </children>
    </Pane>
    <VBox fx:id="vBoxViewData" layoutX="485.0" layoutY="174.0" prefHeight="474.0" prefWidth="515.0" style="-fx-border-color: #000;">
      <children>
        <HBox alignment="TOP_CENTER" prefHeight="21.0" prefWidth="513.0">
          <children>
            <Label text="View Data">
              <font>
                <Font size="14.0" />
              </font>
            </Label>
          </children>
        </HBox>
        <VBox fx:id="vBoxFilter" prefHeight="76.0" prefWidth="513.0">
          <children>
            <Label text="Filter">
              <VBox.margin>
                <Insets left="10.0" />
              </VBox.margin>
            </Label>
          </children>
        </VBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="36.0" prefWidth="513.0">
          <children>
            <TextField fx:id="txtSearchInput" prefWidth="352.0" promptText="Enter a keyword" />
            <Button fx:id="btnSearch" mnemonicParsing="false" onAction="#searchButtonClicked" text="Search">
              <HBox.margin>
                <Insets left="10.0" right="10.0" />
              </HBox.margin>
            </Button>
            <Button fx:id="btnReset" mnemonicParsing="false" onAction="#resetButtonClicked" text="Reset">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Button>
          </children>
        </HBox>
        <TableView fx:id="tvData" prefHeight="360.0" prefWidth="513.0" VBox.vgrow="SOMETIMES">
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </VBox.margin>
        </TableView>
      </children>
    </VBox>
  </children>
</AnchorPane>
